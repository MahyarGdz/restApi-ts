{
  "name": "restapi-ts",
  "version": "1.0.0",
  "description": "Rest Api with express and TypeORM",
  "scripts": {
    "prebuild": "rimraf dist/",
    "build": "npx tsc",
    "start": " node dist/server.js",
    "start:dev": "npm-run-all prebuild build start",
    "lint": "eslint . --ext .ts ",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "debug": "nodemon --watch '*.ts' --exec 'node -r ts-node/register --env-file=.env' ./src/server.ts",
    "seed": "npx ts-node src/utils/seeder.ts",
    "cleanDb": "npx ts-node src/utils/clean.db.ts"
  },
  "author": "MahyarGoudarzi",
  "license": "MIT",
  "dependencies": {
    "class-validator": "^0.14.1",
    "colorette": "^2.0.20",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "slugify": "^1.6.6",
    "typeorm": "^0.3.20",
    "winston": "^3.12.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.25",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.4"
  }
}
